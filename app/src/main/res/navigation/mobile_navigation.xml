<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_fragment_new163_main">


    <fragment
        android:id="@+id/nav_fragment_new163_main"
        android:name="com.lxldev.relax.news163.TabHostFragment"
        android:label="news163"
        tools:layout="@layout/fragment_tabs">

    </fragment>


    <fragment
        android:id="@+id/nav_fragment_pengfu_main"
        android:name="com.lxldev.relax.pengfu.TabHostFragment"
        android:label="pengfu"
        tools:layout="@layout/fragment_tabs" />


    <fragment
        android:id="@+id/nav_fragment_qiubai_main"
        android:name="com.lxldev.relax.qiubai.TabHostFragment"
        android:label="qiubai"
        tools:layout="@layout/fragment_tabs" />

    <action

        android:id="@+id/nav_action_to_pengfu_flag"
        app:destination="@id/nav_fragment_pengfu_main" />
    <action

        android:id="@+id/nav_action_to_new163_flag"
        app:destination="@id/nav_fragment_new163_main" />

    <action

        android:id="@+id/nav_action_to_qiubai_flag"
        app:destination="@id/nav_fragment_qiubai_main" />
</navigation>
